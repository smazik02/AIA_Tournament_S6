{
  "name": "aia-tournament",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "init-env": "pnpm install && cp .env .env.local && echo Fill out .env.local before proceeding with db:init",
    "dev": "next dev",
    "build": "next build",
    "build:dev": "dotenv -e .env.local -- pnpm build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "db:init": "docker compose up --build -d && pnpm db:generate && pnpm db:push",
    "db:generate": "dotenv -e .env.local -- pnpx prisma generate",
    "db:push": "dotenv -e .env.local -- pnpx prisma db push",
    "db:migrate": "dotenv -e .env.local -- pnpx prisma migrate dev --name",
    "db:studio": "dotenv -e .env.local -- pnpx prisma studio"
  },
  "dependencies": {
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fontsource/roboto": "^5.2.5",
    "@mui/icons-material": "^7.0.1",
    "@mui/material": "^7.0.1",
    "@mui/material-nextjs": "^7.0.0",
    "@mui/x-date-pickers": "^8.3.1",
    "@next/third-parties": "^15.3.3",
    "@prisma/client": "6.5.0",
    "better-auth": "^1.2.5",
    "dayjs": "^1.11.13",
    "next": "15.2.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/eslint-plugin-next": "^15.3.3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.2.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "prettier": "3.5.3",
    "prisma": "^6.5.0",
    "tsx": "^4.19.3",
    "typescript": "^5"
  }
}
